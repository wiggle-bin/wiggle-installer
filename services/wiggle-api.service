[Unit]
Description=WiggleApi service
After=network.target

[Service]
WorkingDirectory=%h/WiggleBin/packages/venv/lib/python3.11/site-packages/wiggle_api
ExecStart=%h/WiggleBin/packages/venv/bin/python3 %h/WiggleBin/packages/venv/lib/python3.11/site-packages/wiggle_api/main.py
Environment="PATH=%h/WiggleBin/packages/venv/bin:%h/WiggleBin/packages/venv/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/local/games:/usr/games"

[Install]
WantedBy=default.target